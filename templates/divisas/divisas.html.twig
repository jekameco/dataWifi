
{% extends "base.html.twig" %}

{% block title %}<title>Listado de Calificaciones</title>{% endblock %}

{% block body %}


<div class="container">
    <div class="row">

        {{ form_start(form_divisa, {'attr': {'id': 'form_divisa'}}) }}
        <div class="form-group col-lg-4">

            {{ form_row(form_divisa.date_divisa) }}
        </div>
        <div class="form-group col-lg-4">
            {{ form_widget(form_divisa.submit) }}
        </div>
        {{ form_end(form_divisa) }}
        
    <table class="table table-dark mt-5">
        <thead>
          <tr>
            <th scope="col">Moneda</th>
            <th scope="col">Valor</th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>

    </div>
</div>

{% endblock %}
{% block javascripts %}
<script src="{{ asset('js/divisas.js') }}"></script>

{% endblock %}